Int main()
{
	Int i;
	Note g;	
	Note f;
	Note e;
	Note d;
	Note c;
	Seq s;

	s = $([(60,60|4)]);
	i = 0;

	while (i < 12)
	{
		g = (67 + i,64|1);
		f = (65 + i,64|1);
		e = (64 + i,64|1);
		d = (62 + i,64|1);
		c = (60 + i,64|1);

		s = s + [g];
		s = s + [f];
		s = s + [e];
		s = s + [d];
		s = s + [c, e, g];
		s = s + [c, e, g];
		s = s + [c, f, g];
		s = s + [c, f, g];
		s = s + [(60 + i,64|2),(64 + i,64|2),(67 + i,64|2)];
		s = s + [(61 + i,64|2),(65 + i,64|2),(68 + i,64|2)];

		i = ++i;
	}

	prints(s);
	plays(s);
	return 0;
}