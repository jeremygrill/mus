Int main()
{
	Int i;
	Note g;	
	Note f;
	Note e;
	Note d;
	Note c;
	Seq s;

	s = $([(60,60|4)]);
	i = 0;

	while (i < 12)
	{
		g = (67 + i,60|1);
		f = (65 + i,60|1);
		e = (64 + i,60|1);
		d = (62 + i,60|1);
		c = (60 + i,60|1);

		s = s + [(66 + i,20|1)];
		s = s + [(65 + i,30|1)];
		s = s + [(64 + i,40|1)];
		s = s + [(62 + i,50|1)];
		s = s + [c, e, g];
		s = s + [c, e, g];
		s = s + [c, f, g];
		s = s + [c, f, g];
		s = s + [(60 + i,60|2),(64 + i,60|2),(67 + i,60|2)];
		s = s + [(61 + i,80|2),(65 + i,80|2),(68 + i,80|2)];

